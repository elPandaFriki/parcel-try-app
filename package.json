{
  "name": "parcel_app",
  "version": "1.0.0",
  "description": "",
  "source": "public/index.html",
  "scripts": {
    "production": "nodemon server.js",
    "start": "yarn check_types && parcel -p 3000 --lazy --open chrome --config .parcelrc.json",
